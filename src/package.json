{
  "name": "react-d3-webapp-workspace",
  "version": "1.0.0",
  "description": "Backend for Frontend with React SPA and TypeScript",
  "type": "module",
  "main": "dist/backend/index.js",
  "scripts": {
    "start": "node dist/backend/index.js",
    "dev": "tsx run.ts",
    "build": "tsx run.ts build",
    "build:frontend": "webpack --config webpack/webpack.prod.ts",
    "build:backend": "tsc -p tsconfig.backend.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test-coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier -w ."
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-loader": "^10.0.0",
    "browser-sync": "^3.0.4",
    "connect-history-api-fallback": "^2.0.0",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.30.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-webpack-plugin": "^5.0.2",
    "http-proxy-middleware": "^3.0.5",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "neostandard": "^0.12.1",
    "prettier": "^3.6.2",
    "prettier-config-standard": "^7.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "ts-loader": "^9.5.2",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-merge": "^6.0.1",
    "zustand": "^5.0.8"
  }
}
